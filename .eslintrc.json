{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
     "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    },
    "rules": {
        "import/extensions": "off",
        "camelcase":"off",
        "semi":["error", "never"],
        "no-return-await":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-shadow": "off",
        "no-use-before-define": "off",
        "no-mixed-operators": "off"
    }
}
